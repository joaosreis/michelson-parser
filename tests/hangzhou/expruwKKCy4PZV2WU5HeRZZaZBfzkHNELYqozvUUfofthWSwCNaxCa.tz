{ parameter (or (unit %default) (pair %withdraw (address %address) (mutez %amount))) ;
  storage address ;
  code { CAST (pair (or unit (pair address mutez)) address) ;
         { { DUP ; CAR ; DIP { CDR } } } ;
         IF_LEFT
           { DROP ; NIL operation }
           { SENDER ;
             { DIP 2 { DUP } ; DIG 3 } ;
             COMPARE ;
             EQ ;
             IF {} { PUSH string "NOT_ADMIN" ; FAILWITH } ;
             DUP ;
             CDR ;
             BALANCE ;
             SWAP ;
             COMPARE ;
             LE ;
             IF {} { PUSH string "INSUFFICIENT_BALANCE" ; FAILWITH } ;
             DUP ;
             CAR ;
             CONTRACT unit ;
             IF_NONE { PUSH int 26 ; FAILWITH } {} ;
             NIL operation ;
             SWAP ;
             DIG 2 ;
             CDR ;
             UNIT ;
             TRANSFER_TOKENS ;
             CONS } ;
         PAIR } }
