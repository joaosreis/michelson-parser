         CDR ;
         PUSH nat 0 ;
         PUSH mutez 1 ;
         AMOUNT ;
         EDIV ;
         IF_NONE { UNIT ; FAILWITH } {} ;
         CAR ;
         COMPARE ;
         GT ;
         IF {}
            { PUSH string
                   "WrongCondition: sp.fst(sp.ediv(sp.amount, sp.mutez(1)).open_some(message = sp.unit)) > 0" ;
              FAILWITH } ;
         DUP ;
         GET 4 ;
         CONTRACT %transfer (pair address (pair address nat)) ;
         IF_NONE { PUSH int 14 ; FAILWITH } {} ;
         NIL operation ;
         SWAP ;
         PUSH mutez 0 ;
         PUSH nat 3000000000000 ;
         PUSH mutez 1 ;
         AMOUNT ;
         EDIV ;
         IF_NONE { UNIT ; FAILWITH } {} ;
         CAR ;
         MUL ;
         SENDER ;
         SELF_ADDRESS ;
         PAIR 3 ;
         TRANSFER_TOKENS ;
         CONS ;
         SWAP ;
         DUP ;
         DUG 2 ;
         CAR ;
         CONTRACT unit ;
         IF_NONE { PUSH int 20 ; FAILWITH } {} ;
         AMOUNT ;
         UNIT ;
         TRANSFER_TOKENS ;
         CONS ;
         NIL operation ;
         SWAP ;
         ITER { CONS } ;
         PAIR } }
